<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="jsiisnode" path="*.js" verb="*" modules="iisnode" resourceType="File" />
        </handlers>
        <defaultDocument>
            <files>
                <add value="lib/app.js" />
            </files>
        </defaultDocument>
        <directoryBrowse enabled="true" />
        <iisnode nodeProcessCommandLine="&quot;C:\Program Files\nodejs\node.exe&quot;" />
    </system.webServer>
</configuration>
